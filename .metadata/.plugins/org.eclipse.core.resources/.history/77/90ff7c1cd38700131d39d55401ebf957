import acm.program.*;

public class PythagoreanTheorem extends ConsoleProgram {
	public void run() {
		/* You fill this in */
		println("This program finds the c value for the PythagoeranTherom");
		double a = readInt("Please enter a value for a: ");
		double b = readInt("Please enter a value for b: ");
		double x = Math.sqrt(a);
		double y = Math.sqrt(b);
		
		double c = Math.sqrt(x+y); 
		
	}
}
